<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<package name="framework-communtiy-actions" namespace="/public" extends="framework-default">
		
		<action name="list-announce" class="architecture.ee.web.struts2.action.ajax.AnnounceAction">
			<result name="json-success" type="json">
			    <param name="enableGZIP">true</param>
			    <param name="excludeNullProperties">true</param>
			    <!-- <param name="excludeProperties">targetSocialAccounts.*\.socialServiceProvider</param>-->
				<param name="includeProperties">
				  targetAnnounces.*
				</param>
			</result>	
		</action>		
		<action name="get-announce" class="architecture.ee.web.struts2.action.ajax.AnnounceAction">
			<result name="json-success" type="json">
			    <param name="enableGZIP">true</param>
			    <param name="excludeNullProperties">true</param>
				<param name="includeProperties">
				  targetAnnounce.*
				</param>
			</result>	
		</action>			
		
	</package>

		
</struts>