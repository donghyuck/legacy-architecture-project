<?xml version="1.0" encoding="UTF-8"?>
<sql-queryset name="COMMON" description="공통 쿼리문들을 정의하고 있다">

	<sql-query
		name="SELECT_ALL_ENT_APP"
		comment="모든 어플리케이션 정의를 조회"><![CDATA[
		select * from ent_app
      ]]></sql-query>
            	
	<sql-query
		name="INSERT_ENT_APP_PROPERTY"
		comment="어플리케이션 프로퍼티 입력"><![CDATA[		
		INSERT INTO ENT_APP_PROPERTY(ENT_APP_ID, PROPERTY_NAME, PROPERTY_VALUE) 
		VALUES ( ?, ?, ?)
      ]]></sql-query>

	<sql-query
		name="DELETE_ALL_ENT_APP_PROPERTY"
		comment="어플리케이션 프로퍼티 입력"><![CDATA[		
		DELETE ENT_APP_PROPERTY where ENT_APP_ID = ?
      ]]></sql-query>
      
      
	<sql-query
		name="SELECT_ALL_ENT_APP_PROPERTY"
		comment="전체 어플리케이션 프로퍼티 조회" ><dynamic>
		select * from ENT_APP_PROPERTY where ENT_APP_ID = ?
		</dynamic>
      </sql-query>
      
      
    <sql-query
        name="COUNT_ENT_APP_PROPERTY"
        comment="어플리케이션 프로퍼티 데이터 수 조회" ><![CDATA[	
        select count(*) from ENT_APP_PROPERTY where ENT_APP_ID=?
        ]]></sql-query>     
    
    <sql-query
        name="SELECT_TZ_TUTORMONITOR_BY_YEAR"
        comment="테이블 데이터 조회" ><![CDATA[  
        select CONTENT from TZ_TUTORMONITOR where YEAR = ? 
    ]]></sql-query>  
    
     <sql-query
        name="SELECT_SCRIPT"
        comment="테이블 데이터 조회  스크립트" ><![CDATA[  
        
        select * from TZ_TUTORMONITOR ;        
        select count(*) from TZ_TUTORMONITOR ;
        
    ]]></sql-query> 
    
      	     
</sql-queryset>